10 REM Test file containing all MBASIC operator tokens
20 REM Arithmetic operators
30 A=1+2
40 B=5-3
50 C=4*2
60 D=8/2
70 E=2^3
80 F=7\2
90 G=7 MOD 3
100 REM Comparison operators
110 IF A=B THEN PRINT "EQUAL"
120 IF A<>B THEN PRINT "NOT EQUAL"
130 IF A<B THEN PRINT "LESS"
140 IF A>B THEN PRINT "GREATER"
150 IF A<=B THEN PRINT "LESS OR EQUAL"
160 IF A>=B THEN PRINT "GREATER OR EQUAL"
170 REM Logical operators
180 IF NOT A THEN PRINT "NOT"
190 IF A AND B THEN PRINT "AND"
200 IF A OR B THEN PRINT "OR"
210 IF A XOR B THEN PRINT "XOR"
220 IF A EQV B THEN PRINT "EQV"
230 IF A IMP B THEN PRINT "IMP"
240 REM Delimiters
250 PRINT A;B;C
260 PRINT A,B,C
270 X=(A+B)*C
280 PRINT #1,A$
290 PRINT TAB(10);"TABBED"
300 PRINT SPC(5);"SPACED"
310 REM Special forms
320 A$="TEST":B$="MORE"
330 X=&H1F
340 Y=&O17
999 END
