/* Test all ADDRESS comparison operators */
MON1: PROCEDURE(FUNC, PARM) EXTERNAL;
    DECLARE FUNC BYTE, PARM ADDRESS;
END MON1;
DECLARE PRTSTR LITERALLY '9';
PRINTSTR: PROCEDURE(S);
    DECLARE S ADDRESS;
    CALL MON1(PRTSTR, S);
END PRINTSTR;

MAIN: PROCEDURE;
    DECLARE MSG$OK DATA('OK$');
    DECLARE MSG$FAIL DATA('FAIL$');
    DECLARE MSG$CRLF DATA(13, 10, '$');
    DECLARE (A, B) ADDRESS;
    DECLARE PASS BYTE;
    
    PASS = 1;
    
    /* Test LT: 0 < 1 should be TRUE */
    A = 0; B = 1;
    IF NOT (A < B) THEN PASS = 0;
    
    /* Test LT: 1 < 0 should be FALSE */
    A = 1; B = 0;
    IF A < B THEN PASS = 0;
    
    /* Test LT: 1 < 32768 should be TRUE */
    A = 1; B = 8000H;
    IF NOT (A < B) THEN PASS = 0;
    
    /* Test GT: 1 > 0 should be TRUE */
    A = 1; B = 0;
    IF NOT (A > B) THEN PASS = 0;
    
    /* Test GE: 1 >= 1 should be TRUE */
    A = 1; B = 1;
    IF NOT (A >= B) THEN PASS = 0;
    
    /* Test LE: 0 <= 1 should be TRUE */
    A = 0; B = 1;
    IF NOT (A <= B) THEN PASS = 0;
    
    IF PASS <> 0 THEN
        CALL PRINTSTR(.MSG$OK);
    ELSE
        CALL PRINTSTR(.MSG$FAIL);
    CALL PRINTSTR(.MSG$CRLF);
END MAIN;
CALL MAIN;
